{
	"name": "leanware",
	"version": "1.0.0",
	"description": "technical test for leanware",
	"main": "./src/index.ts",
	"scripts": {
		"start": "NODE_ENV=production node dist/src/index.js",
		"dev": "nodemon --watch './**/*.ts' --exec 'ts-node' src/index.ts",
		"build": "rm -rf dist/ && tsc -b",
		"typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
		"migrations:create": "npm run typeorm -- migration:create -n",
		"migrations:generate": "npm run typeorm -- migration:generate -n",
		"migrations:run": "npm run build && npm run typeorm -- migration:run",
		"migrations:drop": "npm run build && npm run typeorm -- schema:drop",
		"migrations:show": "npm run build && npm run typeorm -- migration:show"
	},
	"keywords": [
		"ts",
		"js",
		"nodemon",
		"eslint",
		"prettier"
	],
	"author": "Juan Sebasti√°n Agudelo @juansacdev <juansac.me@gmail.com>",
	"license": "MIT",
	"devDependencies": {
		"@types/bcryptjs": "^2.4.2",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/helmet": "^4.0.0",
		"@types/jsonwebtoken": "^8.5.5",
		"@types/morgan": "^1.9.3",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "^5.2.0",
		"@typescript-eslint/parser": "^5.2.0",
		"eslint": "^8.1.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-prettier": "^4.0.0",
		"nodemon": "^2.0.14",
		"prettier": "^2.4.1",
		"ts-node": "^10.4.0",
		"typescript": "^4.4.4"
	},
	"dependencies": {
		"@fast-csv/format": "^4.3.5",
		"@fast-csv/parse": "^4.3.6",
		"@hapi/boom": "^9.1.4",
		"@hapi/joi": "^17.1.1",
		"@types/joi": "^17.2.3",
		"@types/json2csv": "^5.0.3",
		"@types/multer": "^1.4.7",
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"eslint-import-resolver-typescript": "^2.5.0",
		"express": "^4.17.1",
		"express-fileupload": "^1.2.1",
		"fast-csv": "^4.3.6",
		"helmet": "^4.6.0",
		"joi": "^17.4.2",
		"json2csv": "^5.0.6",
		"jsonwebtoken": "^8.5.1",
		"morgan": "^1.10.0",
		"multer": "^1.4.3",
		"pg": "^8.7.1",
		"reflect-metadata": "^0.1.13",
		"typeorm": "^0.2.38"
	}
}
